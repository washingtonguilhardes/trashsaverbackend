    "start": "./node_modules/.bin/nest start",
    "orm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f cli.ormconfig.json",
    "postinstall": "npx husky install",
    "local:dev": "docker-compose  -f ./docker-compose.dev.yml  up -d && yarn start:dev",
    "nest:g": "nest -p yarn g",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./cli.ormconfig.json ",
    "prdtypeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js  ",
    "posttypeorm": "yarn lint ./src/database/migrations/*"
  },
